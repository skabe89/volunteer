<div class="container row">
  <div  class="col s8 offset-s2">

<%= form_for project do |f| %>
<%= f.hidden_field :community_id, value: current_user.community.id %>
  <br>
  <%= f.label :title %> <br>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :date %> <br>
  <%= f.date_field :date %>
  <br>
  <br>
  <%= f.label "Approximate Hours Needed" %> <br>
  <%= f.text_field :estimated_hours %>
  <br>
  <br>
  <%= f.label "Volunteers Needed" %> <br>
  <%= f.text_field :min_volunteers %>
  <br>
  <br>
  <%= f.label :address %> <br>
  <%= f.text_field :address %>
  <br>
  <br>
  <%= f.label :description %> <br>
  <%= f.text_area :description %>
  <br>
  <br>
  <%= f.submit button_name , :class => "btn green lighten-1 white-text" %>
<% end %>

  </div>
</div>